class Solution:
    def median(self, matrix, R, C):
        lis=[]
        for i in range(R):
           for j in range(C):
               lis.append(matrix[i][j])
        lis.sort()
        val=int((len(lis))/2)
        
        return(lis[val])
        
        
        
R = 3, C = 3
M = [[1, 3, 5], 
     [2, 6, 9], 
     [3, 6, 9]]
Output: 5
